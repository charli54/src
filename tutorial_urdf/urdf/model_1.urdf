<?xml version="1.0"?>
<robot name="model_1">

	<material name="blue">
		<color rgba="0 0.8 0.8 1"/>
	</material>
	<material name="black">
		<color rgba="0 0 0 1"/>
	</material>

	<link name="base_link">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>					
				<box size="0.3 0.2 0.005" />
			</geometry>
			<material name="blue"/>
		</visual>
		<collision>
			<origin xyz="0 0 -0.005" rpy="0 0 0" />
			<geometry>
				<box size="0.3 0.2 0.175" />
			</geometry>
		</collision>
	</link>

	<link name="base_mdl">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>					
				<box size="0.3 0.2 0.005" />
			</geometry>
			<material name="blue"/>
		</visual>
	</link>

	<joint name="base-base_mdl" type="fixed">
		<origin xyz="0 0 0.065"/>	
		<parent link="base_link"/>
		<child link="base_mdl"/>
	</joint>

	<link name="base_top">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>					
				<box size="0.3 0.2 0.005" />
			</geometry>
			<material name="blue"/>
		</visual>
	</link>

	<joint name="base-base_top" type="fixed">
		<origin xyz="0 0 0.165"/>	
		<parent link="base_link"/>
		<child link="base_top"/>
	</joint>

	<link name="right_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="1.57075 0 0" />
			<geometry>					
				<cylinder length="0.02" radius="0.035"/>
			</geometry>
			<material name="black"/>
		</visual>
	</link>

	<joint name="base-right_wheel" type="continuous">
		<origin xyz="0.05 -0.125 0"/>	
		<axis xyz="0 1 0"/>
		<parent link="base_link"/>
		<child link="right_wheel"/>
	</joint>

	<link name="left_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="1.57075 0 0" />
			<geometry>					
				<cylinder length="0.02" radius="0.035"/>
			</geometry>
			<material name="black"/>
		</visual>
	</link>

	<joint name="base-left_wheel" type="continuous">
		<origin xyz="0.05 0.125 0"/>
		<axis xyz="0 1 0"/>	
		<parent link="base_link"/>
		<child link="left_wheel"/>
	</joint>

	<link name="rear_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>					
				<sphere radius="0.01"/>
			</geometry>
			<material name="black"/>
		</visual>
	</link>

	<joint name="base-rear_wheel" type="fixed">
		<origin xyz="-0.19 0 -0.025"/>	
		<parent link="base_link"/>
		<child link="rear_wheel"/>
	</joint>


</robot>